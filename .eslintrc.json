{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "require-jsdoc": 0,
        "new-cap": 0,
        "linebreak-style": 0,
        "no-invalid-this": 0,
        "guard-for-in": 0,
        "import/no-unresolved": 0,
        "import/prefer-default-export": 0
    }
}
